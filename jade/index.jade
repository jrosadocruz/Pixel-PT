doctype html
html.no-js(lang='en')
  head
    title Pixel Perfect Tree
    meta(name='description', content='We make meaningful web experiences powered by cutting edge technologies for innovative companies and startups.')
    meta(name='keywords', content='Pixel Perfect Tree Web Agency')
    meta(name='author', content='Pixel Perfect Tree')
    meta(property='og:url', content='http://www.pixelpt.com/')
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    link(rel='shortcut icon', href='../favicon.ico')
    link(type='text/css', rel='stylesheet', href='css/style.css')
    script(src="js/min/jquery.min.js")
    script(src="js/min/js-min.js")

  body
    <!--[if gt IE 8]>
    <p class="browsehappy">Está utilizando un navegador <em>desactualizado</em>. Por favor <a target="_blank" href="http://browsehappy.com/">actualize su navegador</a> para mejorar su experiencia dentro de la página web.</p>
    <!<![endif]-->
    header.header.fixed.closed.animate.cf
      a.logo(href='index.html')
        img.logo__img.logo__img--1(src="images/logo.png")
        img.logo__img.logo__img--2(src="images/logo-white.png")
      nav.nav
        ul.nav__list
          li.nav__list-item: a.nav__link.active(href='javascript:void(0)') Home
          li.nav__list-item: a.nav__link(href='javascript:void(0)') Agency
          li.nav__list-item: a.nav__link(href='javascript:void(0)') Contact Us
      button.js-nav-toggle(type='button')
        span.nav-toggle__lines(aria-hidden='true')

    .hero
      .hero__content
        h1
          |Development is our discipline.
          br
          |Detail is our craft.
        p We make meaningful web experiences powered by cutting edge technologies for innovative companies and startups.
      .hero__background.hero__background--home
        img.hero__image(src="images/home/home.jpg")

